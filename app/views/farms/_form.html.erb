<%= semantic_form_for @farm do |f| %>
<%= render "shared/error_messages", :object => f.object%>
  <%= f.inputs do %>
<br /><br>
   <%= f.input :farmname, :label => "Farm name"%>
 
   <%= f.input :total_acres, :label => "Total acres"  %>
  <% end %>
<br />
  <%= f.buttons do %>
    <%= f.commit_button :label => "Update Farm", :button_html => { :class => "button" }%>
  <% end %>
<br />
<% end %>
