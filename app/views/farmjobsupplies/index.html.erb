<h1>Job supplies</h1>

<table>
  <tr>
    <th>User</th>
    <th>Farmjob</th>
    <th>Supply</th>
    <th>Actual qty</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @farmjobsupplies.each do |farmjobsupply| %>
  <tr>
    <td><%= farmjobsupply.user_id %></td>
    <td><%= farmjobsupply.farmjob_id %></td>
    <td><%= farmjobsupply.supply_id %></td>
    <td><%= farmjobsupply.actual_qty %></td>
    <td><%= link_to 'Show', farmjobsupply %></td>
    <td><%= link_to 'Edit', edit_farmjobsupply_path(farmjobsupply) %></td>
    <td><%= link_to 'Destroy', farmjobsupply, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

