<%= semantic_form_for @equipmentactivity do |f| %>
  <%= f.inputs do %>
    <%= f.input :equipment_id %>
    <%= f.input :user_id %>
    <%= f.input :activity %>
    <%= f.input :activity_date %>
    <%= f.input :service_cost %>
    <%= f.input :service_notes %>
  <% end %>

  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
