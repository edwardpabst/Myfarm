<h1>Parties</h1>

<table class="index">
 
  <tr>
 
  
    
    <th>Party name</th>
 
    <th>City</th>
    <th>State</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Job title</th>
    <th></th>
    <th></th>
  </tr>

<% @parties.each do |party| %>
  <tr>
	<td><%= party.partyname %></td>
      
    <td><%= party.partycity %></td>
    <td><%= party.partystate %></td>
    <td><%= party.partyphone %></td>
    <td><%= party.partyemail %></td>
    <td><%= party.partyjobtitle %></td>
    <td><%= link_to 'Edit', edit_party_path(party) %></td>
    <td><%= link_to 'Destroy', party, :confirm => 'Are you sure you want to delete this person?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br>

<%= link_to 'New Party', new_party_path %>
